{
  "name": "portfolio",
  "version": "2.1.0",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "prettier": "prettier --write 'src/**/*.{tsx,ts,scss}'",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint 'src/**/*.ts'",
    "stylelint": "stylelint 'src/**/*.scss' --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "gatsby": "2.18.15",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-layout": "^1.1.18",
    "gatsby-plugin-manifest": "^2.2.33",
    "gatsby-plugin-offline": "^3.0.29",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sharp": "2.3.9",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-source-graphql": "^2.1.29",
    "gatsby-transformer-json": "^2.2.22",
    "gatsby-transformer-sharp": "^2.3.9",
    "graphcms-image": "^1.1.0-beta4",
    "localStorage": "^1.0.4",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-delay": "^0.1.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-normalize": "^0.4.3",
    "react-transition-group": "^4.3.0",
    "react-viewport-hooks": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/react-transition-group": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-stylelint": "^3.2.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "npm run prettier",
      "npm run eslint",
      "git add"
    ],
    "src/**/*.{sass}": [
      "npm run prettier",
      "npm run stylelint",
      "git add"
    ]
  }
}
