{
  "name": "portfolio",
  "version": "2.0.0",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "prettier": "prettier --write 'src/**/*.{js,scss}'",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "stylelint 'src/**/*.js' --fix",
    "stylelint": "stylelint 'src/**/*.scss' --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "gatsby": "^2.3.31",
    "gatsby-image": "^2.0.40",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-source-graphql": "^2.0.18",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.18",
    "localStorage": "^1.0.4",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-delay": "^0.1.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-normalize": "^0.4.0",
    "react-transition-group": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-eslint": "^2.0.4",
    "gatsby-plugin-stylelint": "^3.2.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,scss}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
