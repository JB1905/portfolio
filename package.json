{
  "name": "portfolio",
  "version": "3.0.0",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://$GITHUB_TOKEN@github.com/JB1905/portfolio.git",
    "prettier": "prettier --write 'src/**/*.{tsx,ts,scss,json}'",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint 'src/**/*.ts'",
    "stylelint": "stylelint 'src/**/*.scss' --fix",
    "test": "echo 'Run your tests here'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "gatsby": "2.23.6",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-layout": "^1.3.4",
    "gatsby-plugin-manifest": "^2.4.12",
    "gatsby-plugin-offline": "^3.2.11",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-plugin-sharp": "2.6.12",
    "gatsby-plugin-typescript": "^2.4.6",
    "gatsby-source-filesystem": "^2.3.12",
    "gatsby-source-graphql": "^2.5.5",
    "gatsby-transformer-json": "^2.4.5",
    "gatsby-transformer-sharp": "^2.5.5",
    "graphcms-image": "^1.1.0-beta4",
    "gsap": "^3.3.3",
    "i18next": "^19.5.0",
    "i18next-browser-languagedetector": "^5.0.0",
    "node-sass": "^4.14.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.0",
    "react-normalize": "^0.4.4",
    "react-transition-group": "^4.4.1",
    "s-trimmer": "^1.0.0"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gatsby-plugin-eslint": "^2.0.8",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "npm run prettier",
      "npm run eslint"
    ],
    "src/**/*.{sass}": [
      "npm run prettier",
      "npm run stylelint"
    ]
  }
}
