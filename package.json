{
  "name": "portfolio",
  "version": "3.1.0",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://$GITHUB_TOKEN@github.com/JB1905/portfolio.git",
    "prettier": "prettier --write 'src/**/*.{tsx,ts,scss,json}'",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint 'src/**/*.{tsx,ts}'",
    "stylelint": "stylelint 'src/**/*.scss' --fix",
    "commit": "git-cz",
    "test": "echo 'Run your tests here'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "gatsby": "2.28.2",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-layout": "^1.6.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sass": "^2.7.0",
    "gatsby-plugin-sharp": "2.10.1",
    "gatsby-plugin-typescript": "^2.8.0",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-source-graphql": "^2.10.0",
    "gatsby-transformer-json": "^2.7.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "graphcms-image": "^1.1.0-beta4",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "react": "^17.0.1",
    "react-delay": "^0.1.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.3",
    "react-normalize": "^0.4.4",
    "react-transition-group": "^4.4.1",
    "s-trimmer": "^1.0.0"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "npm run prettier",
      "npm run eslint"
    ],
    "src/**/*.scss": [
      "npm run prettier",
      "npm run stylelint"
    ]
  }
}
