{
  "name": "portfolio",
  "version": "4.2.0",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prettier": "prettier --write 'src/**/*.{tsx,ts,scss,json}'",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint 'src/**/*.{tsx,ts}'",
    "stylelint": "stylelint 'src/**/*.scss' --fix",
    "commit": "git-cz",
    "test": "echo 'Run your tests here'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "gatsby": "3.5.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-image": "^1.1.1",
    "gatsby-plugin-layout": "^2.1.0",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sass": "^4.1.0",
    "gatsby-plugin-sharp": "3.1.1",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-source-graphql": "^3.1.0",
    "gatsby-transformer-json": "^3.1.0",
    "gatsby-transformer-sharp": "^3.1.0",
    "graphcms-image": "^1.1.0-beta4",
    "i18next": "^20.0.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "react": "^17.0.2",
    "react-delay": "^0.1.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.10",
    "react-normalize": "^0.4.4",
    "react-transition-group": "^4.4.1",
    "s-trimmer": "^1.0.0"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "gatsby-plugin-eslint": "^3.0.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "npm run prettier",
      "npm run eslint"
    ],
    "src/**/*.scss": [
      "npm run prettier",
      "npm run stylelint"
    ]
  }
}
