{
  "name": "portfolio",
  "version": "2.1.1",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://$GITHUB_TOKEN@github.com/JB1905/portfolio.git",
    "prettier": "prettier --write 'src/**/*.{tsx,ts,scss}'",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint 'src/**/*.ts'",
    "stylelint": "stylelint 'src/**/*.scss' --fix",
    "test": "echo 'Run your tests here'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "gatsby": "2.19.22",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-layout": "^1.1.22",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-source-graphql": "^2.1.33",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-sharp": "^2.3.15",
    "graphcms-image": "^1.1.0-beta4",
    "localStorage": "^1.0.4",
    "node-sass": "^4.13.1",
    "react-delay": "^0.1.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-normalize": "^0.4.4",
    "react-transition-group": "^4.3.0",
    "react-viewport-hooks": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-transition-group": "^4.2.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-stylelint": "^3.2.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "npm run prettier",
      "npm run eslint"
    ],
    "src/**/*.{sass}": [
      "npm run prettier",
      "npm run stylelint"
    ]
  }
}
